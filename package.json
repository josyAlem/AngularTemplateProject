{
  "name": "angular-template-proj",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "start:prod": "http-server ./dist/ang-proj -a localhost -p 4200",
    "build-start": "run-s build:prod start:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "server": "nodemon ./backend/server.js",
    "data-server": "json-server --watch ./data/db.json",
    "build-image": "docker image build -t josyalem/angular-template:v1 .",
    "remove-image": "docker image rm josyalem/angular-template:v1",
    "dockerize": "run-s build:prod build-image",
    "run-container": "docker run -d --rm -it --name ang-template -p 127.0.0.1:5050:80  josyalem/angular-template:v1",
    "stop-container": "docker container stop ang-template",
    "remove-container": "docker container rm ang-template",
    "cleanup-container": "run-s stop-container remove-container",
    "container-terminal": "docker exec -it ang-template /bin/sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "angular-in-memory-web-api": "^0.11.0",
    "class-transformer": "^0.4.0",
    "express": "^4.17.1",
    "json-server": "^0.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "studio-ui-tmpl": "0.0.5",
    "tslib": "^2.3.0",
    "underscore": "^1.13.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.3",
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/underscore": "^1.11.3",
    "css-loader": "^6.5.1",
    "http-server": "^13.0.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.3.5"
  }
}
